{
  "name": "vue-ts-h5",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:mock": "vite --open --mode mock",
    "dev": "cross-env NODE_ENV=development vite --host --mode local_dev",
